{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "acrName": {
      "value": "<Your ACR Name>"
    },
    "appServicePlanName": {
      "value": "<Your App Service Plan Name>"
    },
    "webAppName": {
      "value": "<Your Web App Name>"
    },
    "location": {
      "value": "North Europe"
    },
    "containerRegistryName": {
      "value": "<Your Container Registry Name>"
    },
    "containerRegistryImageName": {
      "value": "<Your Container Registry Image Name>"
    },
    "containerRegistryImageVersion": {
      "value": "<Your Container Registry Image Version>"
    },
    "dockerRegistryUrl": {
      "value": "<Your Docker Registry URL>"
    },
    "dockerRegistryUsername": {
      "value": "<Your Docker Registry Username>"
    },
    "dockerRegistryPassword": {
      "value": "<Your Docker Registry Password>"
    }
  }
}
